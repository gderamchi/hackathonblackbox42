{
  "enabled": true,
  "auto_comment": true,
  "severity_threshold": "low",
  "ignore_patterns": [
    "*.md",
    "*.txt",
    "package-lock.json",
    "yarn.lock",
    "*.min.js",
    "*.min.css"
